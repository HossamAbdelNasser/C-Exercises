#include <stdio.h>

char *month_name(int n) {
    
    char *months[] = {"Illegal month",
"January", "February", "March",
"April", "May", "June",
"July", "August", "September",
"October", "November", "December"};

return (n < 0 || n > 12) ? months[0] : months[n];

}

int main() {
    
    printf("%s", month_name(12));

    return 0;
}
