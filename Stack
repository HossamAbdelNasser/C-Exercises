#include <stdio.h>

int stack[100];
int sp = 0; //stack pointer

int push(int x) {
    if (sp == 100) 
        printf("Stack overflow\n");

    stack[sp] = x;
    sp++;
}

int pop() {
    int res = stack[sp-1];
    sp--;
    return res;
}

int main() {
    push(1);
    push(2);
    push(3);
    push(8);

for (int i =0; i < 4; i++)
    printf("%d\n", pop());

    return 0;
}
